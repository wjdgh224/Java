import java.math.*;

public class practice {
  public static void array() {
    int [][] n = new int[4][4];
    
    for(int i=0; i<n.length; i++) {
      for(int j=0; j<n[i].length; j++) {
        n[i][j] = (int)(10*Math.random()+1);
        System.out.print(n[i][j] + " ");
      }
      System.out.print("\n");
    }
  }
  public static void main(String[] args) {
    array();
  }
}
